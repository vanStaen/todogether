{
  "name": "todogether.com",
  "version": "1.1.0",
  "keywords": [
    "todogether",
    "todo",
    "todoList"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanStaen/todogether.git"
  },
  "author": "cvanstaen",
  "homepage": "https://github.com/vanStaen/todogether",
  "scripts": {
    "devfr": "webpack serve --config webpack.config.dev.cjs --hot --open",
    "dev": "nodemon server/index.js",
    "server": "npm run build; pm2 start server/index.js",
    "start": "node server/index.js",
    "prebuild": "rm -rf ./build",
    "build": "webpack --config webpack.config.prod.cjs --mode production",
    "deploy": "npm i; heroku git:remote -a todogether-cvs; git push -f heroku master; openprodurl",
    "postdeploy": "rm -rf ./build",
    "log": "heroku logs --tail",
    "version": "heroku --version; git --version",
    "kill": "killall -9 node",
    "eslint": "npx eslint --fix src",
    "openprodurl": "open https://www.todogether.com"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@ant-design/compatible": "^5.1.2",
    "@ant-design/icons": "^5.5.2",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "antd": "^5.23.0",
    "aws-sdk": "^2.1261.0",
    "axios": "^1.2.0",
    "babel-loader": "^8.2.5",
    "bcryptjs": "^2.4.3",
    "cookie-session": "^2.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.23.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "fs": "^0.0.1-security",
    "graphql": "^15.8.0",
    "html-webpack-plugin": "^5.5.0",
    "i18next": "^21.8.14",
    "i18next-browser-languagedetector": "^6.1.4",
    "jest": "^28.1.3",
    "jsonwebtoken": "^9.0.2",
    "mobx": "^6.6.1",
    "mobx-react": "^7.5.2",
    "moment": "^2.29.4",
    "node-html-parser": "^7.0.1",
    "path": "^0.12.7",
    "pg": "^8.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.1",
    "react-router-dom": "^6.3.0",
    "regenerator-runtime": "^0.13.9",
    "sequelize": "^6.25.8",
    "sequelize-cli": "^6.5.2",
    "universal-cookie": "^7.2.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^15.14.0",
    "less-loader": "^12.2.0",
    "nodemon": "^3.1.9",
    "style-loader": "^3.3.1",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.9.3"
  },
  "type": "module"
}
